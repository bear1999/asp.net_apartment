
@{
    ViewBag.Title = "Đăng ký dịch vụ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0 text-dark">Đăng ký dịch vụ</h1>
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-auto">
                <div class="card card-widget">
                    <div class="card-body">
                        <b><span style="color:Highlight">@ViewData["Err1"]</span></b>
                        <br /><b><span>@ViewData["Thongtin1"]</span></b>
                        <br /><b><span>@ViewData["Thongtin2"]</span></b>
                        <br /><b><span>@ViewData["Thongtin3"]</span></b>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
